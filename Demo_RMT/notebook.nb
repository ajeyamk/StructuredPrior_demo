     "step_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"zvalues", ",", "bulkvalues", ",", "pos0"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"zvalues", "=", 
       RowBox[{"Range", "[", 
        RowBox[{"zinf", ",", "zmax", ",", "step"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"We", " ", "remove", " ", "z"}], " ", "=", " ", 
        RowBox[{"0", " ", "for", " ", "numerical", " ", "issues"}]}], "*)"}], 
      " ", "\[IndentingNewLine]", 
      RowBox[{"pos0", "=", 
       RowBox[{"Position", "[", 
        RowBox[{"zvalues", ",", "0"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"pos0", "\[NotEqual]", 
         RowBox[{"{", "}"}]}], ",", 
        RowBox[{"zvalues", "=", 
         RowBox[{"Delete", "[", 
          RowBox[{"zvalues", ",", " ", 
           RowBox[{"pos0", "[", 
            RowBox[{"[", 
             RowBox[{"1", ",", "1"}], "]"}], "]"}]}], "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"bulkvalues", "=", 
       RowBox[{"Table", "[", 
