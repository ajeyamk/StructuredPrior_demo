 FractionBox[
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"s", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", "3"}], "+", 
        RowBox[{"4", " ", "s"}]}], ")"}]}], "+", 
