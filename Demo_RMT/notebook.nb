 RowBox[{"FullSimplify", "[", 
  RowBox[{"coeffT12", ",", 
   RowBox[{
    RowBox[{"s", "<", "0"}], "&&", 
    RowBox[{"\[CapitalDelta]", ">", "0"}]}]}], "]"}]], "Input",
